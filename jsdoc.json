{
  "tags": {
      "allowUnknownTags": true
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.[t|j]s(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
      "plugins/markdown"
  ],
  "opts": {
      "encoding": "utf8",
      "destination": "docs/",
      "recurse": true,
      "verbose": true
  }
}
