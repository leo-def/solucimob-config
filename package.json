{
  "name": "solucimob-config",
  "version": "1.1.0",
  "description": "Serviço de configuração para a solução imobiliária",
  "main": "src/Server.ts",
  "scripts": {
    "localdb": "md .\\db\\mongodb\\dev & mongod --dbpath .\\db\\mongodb\\dev",
    "start": "node dist/Server.js",
    "start:lw": "node --optimize_for_size --max_old_space_size=920 --gc_interval=100 dist/Server.js",
    "start:xlw": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 dist/Server.js",
    "dev": "npx ts-node-dev --respawn --transpile-only --watch src src/Server.ts",
    "dev:localdb": "npx npm-run-all -p localdb dev",
    "debug": "npx ts-node-dev  --inspect --respawn --ignore-watch node_modules --no-notify --transpile-only src/Server.ts",
    "debug:localdb": "npx npm-run-all -p localdb debug",
    "test": "npx jest --no-cache --passWithNoTests",
    "build": "npx babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "build:ts": "npx tsc",
    "docs": "npx jsdoc -c jsdoc.json",
    "lint": "npx eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leo-def/solucimob-config.git"
  },
  "keywords": [
    "solução",
    "imobiliária",
    "configuração"
  ],
  "author": "Leonardo de Freitas Oliveira <leo_defoliveira@live.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leo-def/solucimob-config/issues"
  },
  "homepage": "https://github.com/leo-def/solucimob-config#readme",
  "dependencies": {
    "@types/swagger-ui-express": "^4.1.2",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-openapi-validate": "^0.5.1",
    "helmet": "^4.4.1",
    "lodash": "^4.17.20",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.33",
    "mongoose": "^5.11.15",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "supertest": "^6.1.3",
    "swagger-ui-express": "^4.1.6",
    "typescript": "^4.1.5",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-typescript": "^7.12.16",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "babel-jest": "^26.6.3",
    "eslint": "^7.19.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.5.1",
    "ts-node-dev": "^1.1.1",
    "tsc": "^1.20150623.0"
  }
}
